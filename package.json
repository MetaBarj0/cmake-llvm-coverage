{
  "name": "cpp-llvm-coverage",
  "displayName": "C++ file code coverage with Llvm",
  "description": "Visual code coverage indicators for C++ files using Llvm and cmake capabilities.",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.52.0"
  },
  "categories": [
    "Programming Languages",
    "Linters"
  ],
  "publisher": "SbastienLevy",
  "repository": {
    "url": "https://github.com/MetaBarj0/cpp-llvm-coverage"
  },
  "keywords": [
    "c++",
    "tdd",
    "unit",
    "test",
    "coverage",
    "cmake",
    "llvm",
    "cpp",
    "lint"
  ],
  "activationEvents": [
    "onCommand:cpp-llvm-coverage.helloWorld"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cpp-llvm-coverage.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "package": "npm run compile && vsce package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "deploy": "npm run package && vsce publish -p"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.52.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "typescript": "^4.1.2",
    "vsce": "^1.84.0",
    "vscode-test": "^1.4.1"
  }
}